{
  "name": "makemula.ai",
  "version": "2.40.2",
  "main": "index.js",
  "scripts": {
    "test": "true",
    "start": "node app.js",
    "create-encore": "node ./scripts/debug/createEncore.js",
    "version": "auto-changelog -p -l false --template keepachangelog && git add . && git commit -m \"chore(release): update CHANGELOG.md\"",
    "postversion": "git tag www-v$(npm pkg get version | tr -d '\"')",
    "postinstall": "npx puppeteer browsers install chrome",
    "heroku-postbuild": "mkdir ./.cache && mv /app/.cache/puppeteer ./.cache",
    "mulize-extract": "node scripts/extracts/run.js --source athena --extract 1HourPageViewByHost",
    "mulize-build": "node scripts/transform-load/mulize/build/index.js",
    "mulize-publish": "node scripts/transform-load/mulize/publish/index.js",
    "reports-build": "node scripts/extracts/run.js --source athena --extract 14DayReport && node scripts/extracts/run.js --source athena --extract 7DayNetworkReport",
    "reports-publish": "node scripts/transform-load/reports/index.js",
    "mulize-manifest": "node scripts/transform-load/mulize/manifest/index.js",
    "queries:list": "node queries/cli.js list",
    "queries:info": "node queries/cli.js info",
    "queries:run": "node queries/cli.js run",
    "queries:schedule": "node queries/cli.js schedule",
    "queries:schedule:start": "node queries/cli.js schedule start",
    "queries:schedule:stop": "node queries/cli.js schedule stop",
    "queries:schedule:list": "node queries/cli.js schedule list",
    "queries:schedule:create": "node queries/cli.js schedule create",
    "queries:schedule:delete": "node queries/cli.js schedule delete",
    "queries:schedule:run-now": "node queries/cli.js schedule run-now",
    "queries:test": "node queries/test.js",
    "next-page-test": "node scripts/test-next-page-query.js",
    "refresh-next-page-manifests": "node scripts/next-page-manifest-refresh.js",
    "test-next-page-targeting": "node scripts/test-next-page-targeting-local.js",
    "test-manifest-builder-on3": "node scripts/test-manifest-builder-on3.js",
    "top-products": "node queries/runners/top-products.js --days-back 30",
    "top-products:dev": "node queries/runners/top-products.js --days-back 7",
    "test-site-targeting": "node scripts/test-site-targeting.js",
    "anomaly-analysis": "node queries/runners/september-11-anomaly-analysis.js",
    "experiment:density-position": "node queries/runners/smartscroll-density-position-experiment.js --days-back 14",
    "experiment:density-position:dev": "node queries/runners/smartscroll-density-position-experiment.js --days-back 7",
    "experiment:density-position:cached": "node queries/runners/smartscroll-density-position-experiment.js --use-cached",
    "experiment:button": "node queries/runners/smartscroll-button-experiment.js --days-back 7",
    "experiment:button:cached": "node queries/runners/smartscroll-button-experiment.js --use-cached"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "@slack/web-api": "^7.9.1",
    "aws-sdk": "^2.1692.0",
    "basic-auth": "^2.0.1",
    "bull": "^4.16.5",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-handlebars": "^8.0.1",
    "jsdom": "^26.0.0",
    "node-cron": "^3.0.3",
    "openai": "^4.78.1",
    "paapi5-nodejs-sdk": "^1.0.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^24.1.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "raw-body": "^3.0.0",
    "sequelize": "^6.37.5",
    "serpapi": "^2.1.0",
    "unsplash-js": "^7.0.19",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "auto-changelog": "^2.5.0"
  }
}
